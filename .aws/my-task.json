{
  "family": "my-task",
  "containerDefinitions": [
    {
      "name": "my-container",
      "image": "docker.io/nginx:latest",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "DB_HOST",
          "value": "localhost"
        },
        {
          "name": "DB_NAME",
          "value": "tempfile"
        },
        {
          "name": "DB_USER",
          "value": "postgres"
        },
        {
          "name": "DB_PASSWORD",
          "value": "postgres"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "PORT",
          "value": "3001"
        },
        {
          "name": "FRONTEND_URL",
          "value": "http://localhost:3000"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::897722705947:role/my-ecs-task-role",
  "executionRoleArn": "arn:aws:iam::897722705947:role/my-ecs-execution-task-role",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "cpu": "256",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
